void input() {
FILE *file = fopen("table.dat", "r");
for (int i = 0; i < 500; i++) {
 fread(&db[i], sizeof db[i], 1, file);
countRecords++
}
fclose(file);
}

void saveRecord() {
FILE *f = fopen("table.dat", "a");

countRecords++;
fclose(f);
}

