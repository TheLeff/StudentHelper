struct OLS {
Table element;
OLS *next;
OLS *last
};

void createOLS() {
OLS *element, *current, *pred;
*FILE A = fopen"table.dat", "rb");

first = NULL;
while(fread(&listh, sizeof(listh), 1, A)) {
element = new (OLS);
element->db=listh;
element->next=NULL;
if (first == NULL) first = element;
else {
current = first;
pred = NULL;
while ((current != NULL))
{
pred = current;
current = current->next;
}
if (current == first)
{
element->next = first;
first = element;
}

else
{
element->next = pred->next;
pred->next = element;
}
}
}
fclose(A);
}

void printOLS(OLS *current) {
OLS *tek;
tek = current;
while (tek != NULL)
{
cout << endl << tek->asd " "<< tek->zxc " "<< tek->awt " "<< tek->gasg " "<< tek->asxzx " "<< tek->gasg " ";
tek=tek->next;
}
}

struct OLS {
Table element;
OLS *next;
};
